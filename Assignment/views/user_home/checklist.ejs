<!DOCTYPE html>
<html>
<head>
	<title>Country List</title>
</head>
<body>
	<h1> Country List</h1>

	

	<table border="1">
		<tr>
			<td>Country</td>
			<td>Place</td>
			<td>Travel Agency</td>
			<td>Cost</td>
			<td>Contact</td>
			<td>Action</td>
		</tr>

		<% 
			checklist.forEach( function(check){ %>
			<tr>
				<td><%= check.country %></td>
				<td><%= check.place %></td>
				<td><%= check.travel_agency %></td>
				<td><%= check.cost %></td>
				<td><%= check.contact %></td>

				<td>
					<a href="/scout_home/unlike/<%= check.place %>">Unlike</a> |
				</td>
			</tr>

		<%
			}); 
		%>
		
	</table>
	<br>
	<button><a href="/user_home">Back</a></button>
	<button><a href="/logout">logout</a> </button>
	

</body>
</html>